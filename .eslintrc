{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": true,
    "jsx": false
  },
  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": 0,
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 2
  },
  "env": {
    "node": true,
    "browser": true,
    "meteor": true,
  },
  "globals": {
    "IS_SERVER": true,
    "google": true,
    "$": true,
    "Swiper": true,
    "Meteor": true,
    "Slideout": true,
    "Transitioner": true,
    "GoogleMaps": true,
    "Waves": true,
    "AccountsTemplates": true,
    "LaunchScreen": true,
    "AdminConfig": true,
    "moment": true,
    "Snap": true,
    "L": true,
    "BotanikaMap": true,
    "styles": true,
    "paths": true,
    "TweenLite": true,
    "MorphSVGPlugin": true,

    "Pages": true,
    "Houses": true,
    "News": true,
    "Attachments": true,
    "NewsPictures": true,
  }
}
